<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="9dp"
        android:height="2dp"
        android:top="6dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="15dp"
        android:height="2dp"
        android:top="5dp"
        android:left="5dp">
        <rotate
            android:fromDegrees="-46">
            <shape android:shape="rectangle">
                <solid android:color="@android:color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="10dp"
        android:height="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>